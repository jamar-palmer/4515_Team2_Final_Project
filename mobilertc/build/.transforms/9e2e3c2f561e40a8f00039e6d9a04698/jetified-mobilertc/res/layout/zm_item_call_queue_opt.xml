<?xml version="1.0" encoding="utf-8"?>
<us.zoom.androidlib.widget.ZMSettingsCategory xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:zm_showTopDivider="false"
    android:orientation="vertical">

    <LinearLayout
        style="@style/ZmSettingItem"
        android:id="@+id/optionView">

        <TextView
            android:id="@+id/callQueueName"
            style="@style/ZmSettingItemTitle"
            android:textColor="@color/zm_v2_txt_primary_color"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:maxLines="1"
            tools:text="Call Queue Name"
            android:duplicateParentState="true"/>

        <us.zoom.androidlib.widget.ZMCheckedTextView
            android:id="@+id/chkReceiveCallsFromSpecificCallQueue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checkMark="@drawable/zm_btn_toggle_default"
            android:clickable="false"
            android:gravity="center"
            android:duplicateParentState="true"/>
    </LinearLayout>
</us.zoom.androidlib.widget.ZMSettingsCategory>