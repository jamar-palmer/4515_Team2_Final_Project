<?xml version="1.0" encoding="utf-8"?>
<com.zipow.annotate.annoMultiPage.RectangleItemLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/zm_v1_white_alpha0">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/show_whiteboard"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:background="@drawable/zm_bg_wb_item_onlight"
            android:padding="5dp"
            android:scaleType="centerCrop"
            tools:ignore="ContentDescription" />
    </LinearLayout>

    <ImageView
        android:id="@+id/btn_del_whiteboard"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        android:clickable="true"
        android:scaleType="centerInside"
        android:src="@drawable/zm_btn_del_whiteboard"
        android:contentDescription="@string/zm_btn_delete"
        android:focusable="true" />

    <ImageView
        android:id="@+id/btn_check_whiteboard"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        android:clickable="true"
        android:scaleType="centerInside"
        android:src="@drawable/zm_anno_wb_check"
        android:visibility="gone"
        android:focusable="true"
        android:contentDescription="@string/zm_accessibility_icon_item_selected_19247"/>


</com.zipow.annotate.annoMultiPage.RectangleItemLayout>
