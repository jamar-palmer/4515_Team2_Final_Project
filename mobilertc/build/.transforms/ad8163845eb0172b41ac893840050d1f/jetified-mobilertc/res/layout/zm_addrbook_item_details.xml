                    android:layout_marginLeft="0dp"
                    android:contentDescription="@string/zm_description_mm_btn_more"
                    android:src="@drawable/zm_ic_more_ondark" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/txtTitle"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical" >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <us.zoom.androidlib.widget.ZMEllipsisTextView
                        android:id="@+id/txtScreenName"
                        style="@style/ZmTitleBarTitle.Dark"
                        android:layout_width="wrap_content"
                        tools:text="aa"/>
                    <ImageView
                        android:id="@+id/robotIcon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/zm_margin_smallest_size"
                        android:visibility="gone"
                        tools:visibility="gone"
                        android:contentDescription="@string/zm_accessibility_robot_icon_289719"
                        android:src="@drawable/zm_mm_adddrbook_robot_title_icon_v2" />
                </LinearLayout>

                <TextView
                    android:id="@+id/txtScreenSubName"
                    style="@style/ZmTextView.Content.Primary.Medium.Dark"
                    tools:text="@tools:sample/lorem"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:visibility="visible"
                    android:visibility="gone" />
            </LinearLayout>
        </us.zoom.androidlib.widget.ZMIOSStyleTitlebarLayout>

