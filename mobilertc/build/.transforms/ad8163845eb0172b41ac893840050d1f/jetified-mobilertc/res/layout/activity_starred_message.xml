<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:zoom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/zm_white"
    tools:context="com.zipow.videobox.StarredMessageActivity">

    <us.zoom.androidlib.widget.ZMIOSStyleTitlebarLayout
        android:id="@+id/panelTitleBar"
        style="@style/ZmTitleBar.Dark"
        zoom:zm_leftButton="@+id/zm_starred_message_title_back_btn"
        zoom:zm_rightButton="@+id/viewRight"
        zoom:zm_title="@+id/txtTitle" >

        <ImageButton
            android:id="@+id/zm_starred_message_title_back_btn"
            style="@style/ZmBackButton.Dark"
            android:contentDescription="@string/zm_btn_back" />

        <LinearLayout
            android:id="@+id/viewRight"
            android:gravity="right"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
        </LinearLayout>

        <us.zoom.androidlib.widget.ZMDynTextSizeTextView
            android:id="@+id/txtTitle"
            style="@style/ZmTitleBarTitle.Dark"
            android:text="@string/zm_mme_starred_message_title_name_274700" />
    </us.zoom.androidlib.widget.ZMIOSStyleTitlebarLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/zm_starred_message_fragment_container">

    </LinearLayout>
</LinearLayout>
