<?xml version="1.0" encoding="utf-8"?>
<us.zoom.androidlib.widget.ZMSettingsCategory xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout style="@style/ZmSettingItem.UnClick">

        <TextView
            android:id="@+id/tvAppName"
            style="@style/ZmSettingItemTitle"
            tools:text="Salesforce" />

        <TextView
            android:id="@+id/tvDuration"
            style="@style/ZmSettingItemContent"
            android:ellipsize="start"
            android:paddingRight="14dp"
            tools:text="1 hour" />

        <ImageView
            style="@style/ZmSettingItemArrow"
            android:layout_marginLeft="@dimen/zm_margin_smaller_size"
            tools:ignore="ContentDescription" />
    </LinearLayout>
</us.zoom.androidlib.widget.ZMSettingsCategory>